<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib
        PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
	"http://java.sun.com/j2ee/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>
	<tlib-version>1.0</tlib-version>
	<jsp-version>1.2</jsp-version>
	<short-name>eap</short-name>
	<uri>eap</uri>
	<tag>
		<name>block</name>
		<tag-class>com.originsys.eap.control.BlockTag</tag-class>
		<attribute>
			<name>action_id</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>block_id</name>
			<required>false</required>
		</attribute>
		<attribute>
			<name>skin_name</name>
			<required>false</required>
		</attribute>
		<attribute>
			<name>view_id</name>
			<required>false</required>
		</attribute>
		<attribute>
			<name>site_id</name>
			<required>false</required>
		</attribute>
		<attribute>
			<name>skin_sequence</name>
			<required>false</required>
		</attribute>
		<attribute>
			<name>block_name</name>
			<required>false</required>
		</attribute>
		<attribute>
			<name>component_name</name>
			<required>false</required>
		</attribute>
		<attribute>
			<name>action_name</name>
			<required>false</required>
		</attribute>
		<attribute>
			<name>block_style</name>
			<required>false</required>
		</attribute>
		<attribute>
			<name>param</name>
			<required>false</required>
		</attribute>
	</tag>
	<tag>
		<name>content</name>
		<tag-class>com.originsys.eap.control.ContentTag</tag-class>
		<attribute>
			<name>view_id</name>
			<required>false</required>
		</attribute>
		<attribute>
			<name>skin_name</name>
			<required>false</required>
		</attribute>
		<attribute>
			<name>skin_sequence</name>
			<required>false</required>
		</attribute>		
	</tag>
</taglib>