package com.originsys.auth.domain;

import java.util.Map;

import com.originsys.eap.domain.User;

/**
 auth:boy 2014-7-31
   描述：发送邮件的对象
 */
public class Email {
	/** 邮件服务器*/
	private String smtp;
	/** smtp认证的用户名*/
	private String authertic_name;
	/** smtp认证的密码*/
	private String authertic_pass;
	/** 用户对象 (用户id 用户名邮箱这个必须 等等)*/
	private User user;
	/** 邮件视图模板 用EmailTemplate常量对象里的值*/
	private String viewFile;
	/** 邮件标题*/
	private String title;
	/**其他参数集合*/
	private Map paramMap;
	public String getSmtp() {
		return smtp;
	}
	/** 邮件服务器*/
	public void setSmtp(String smtp) {
		this.smtp = smtp;
	}
	public String getAuthertic_name() {
		return authertic_name;
	}
	/** smtp认证的用户名*/
	public void setAuthertic_name(String authertic_name) {
		this.authertic_name = authertic_name;
	}
	public String getAuthertic_pass() {
		return authertic_pass;
	}
	/** smtp认证的密码*/
	public void setAuthertic_pass(String authertic_pass) {
		this.authertic_pass = authertic_pass;
	}
	public User getUser() {
		return user;
	}
	/** 用户对象 (用户id 用户名邮箱这个必须 等等)*/
	public void setUser(User user) {
		this.user = user;
	}
	public String getViewFile() {
		return viewFile;
	}
	/** 邮件视图模板 用EmailTemplate常量对象里的值*/
	public void setViewFile(String viewFile) {
		this.viewFile = viewFile;
	}
	public String getTitle() {
		return title;
	}
	/** 邮件标题*/
	public void setTitle(String title) {
		this.title = title;
	}
	public Map getParamMap() {
		return paramMap;
	}
	/**其他参数集合*/
	public void setParamMap(Map paramMap) {
		this.paramMap = paramMap;
	}
	
}
