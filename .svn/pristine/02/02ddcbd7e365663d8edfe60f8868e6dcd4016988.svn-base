package com.originsys.realtygis.domain;

public class Floor {
	private String building_id;//幢号内码
	private String floor_id;//楼层表内码
	private int floor_number;//所在建筑物层数
	private String projectsddfile_id;//成果文件内码
	private String basic_layer;//所在图形基层
	private String layer_name_cn;//层名
	
	public Floor(){
		this.building_id="";
		this.floor_id="";
		this.floor_number=0;
		this.projectsddfile_id="";
		this.basic_layer="";
		this.layer_name_cn="";
	}
	
	public String getBuilding_id() {
		return building_id;
	}
	public void setBuilding_id(String building_id) {
		this.building_id = building_id;
	}
	public String getFloor_id() {
		return floor_id;
	}
	public void setFloor_id(String floor_id) {
		this.floor_id = floor_id;
	}
	public int getFloor_number() {
		return floor_number;
	}
	public void setFloor_number(int floor_number) {
		this.floor_number = floor_number;
	}
	public String getProjectsddfile_id() {
		return projectsddfile_id;
	}
	public void setProjectsddfile_id(String projectsddfile_id) {
		this.projectsddfile_id = projectsddfile_id;
	}
	public String getBasic_layer() {
		return basic_layer;
	}
	public void setBasic_layer(String basic_layer) {
		this.basic_layer = basic_layer;
	}
	public String getLayer_name_cn() {
		return layer_name_cn;
	}
	public void setLayer_name_cn(String layer_name_cn) {
		this.layer_name_cn = layer_name_cn;
	}

}
