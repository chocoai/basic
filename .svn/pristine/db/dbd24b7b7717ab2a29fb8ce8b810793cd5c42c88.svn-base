@charset "utf-8";
/* CSS Document */

 *{ margin:0px; padding:0px; font-size:12px; font-family:"宋体";}
img { border:none;display:block}
a{ text-decoration:none}
ul { list-style:none;}
.clear{ clear:both}
body{ background:#f3f3f3}
h1{ font-size:24px;}

.houseList_wrapper{ width:1000px; margin:0 auto}
.list_logo{ padding-left:360px;}
.list01{ padding-top:15px; padding-bottom:5px; width:859px; margin:0 auto;}
.list_center{ background:#0061cf; width:859px; height:75px;}
.center_left{ width:123px; height:31px; background:url(images/listtitleBg.jpg); float:left}
.numtitle{ font-size:20px; font-weight:bold; color:#0061cf; font-family:"微软雅黑"; padding-top:2px; margin-left:15px;}
.center_right{ float:left; margin-left:10px;}

.num_table{ border-collapse:collapse; width:700px;  color:#fff; }
.num_table td{ border:solid 1px #a0d1f9;}
.num_table a{ color:#FFF; }